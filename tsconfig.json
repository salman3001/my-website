{
  "compilerOptions": {
    "target": "es2016",
    "module": "NodeNext",
    "strict": true,
    "moduleDetection": "force",
    "resolveJsonModule": true,
    "moduleResolution": "NodeNext",
    "allowJs": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "strictPropertyInitialization": false
  }
}
